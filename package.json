{
  "name": "lexpress",
  "version": "0.39.5",
  "description": "NodeJS + Express based light framework.",
  "license": "MIT",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "build": "webpack --display-error-details",
    "test": "npm run validate && mocha -r ts-node/register src/**/*.spec.ts",
    "preversion": "npm test && npm run build",
    "validate": "npm run validate:lint && npm run validate:tsc && npm run validate:sec",
    "validate:lint": "tslint -p tsconfig.json",
    "validate:sec": "nsp check && snyk test",
    "validate:tsc": "tsc --noEmit --pretty",
    "version": "node tasks/injectVersion && git add .",
    "postversion": "npm publish"
  },
  "dependencies": {
    "@inspired-beings/log": "^1.1.3",
    "@types/body-parser": "^1.17.0",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/helmet": "0.0.37",
    "@types/memory-cache": "^0.2.0",
    "@types/mongodb": "^3.0.18",
    "@types/node": "^9.6.12",
    "@types/query-string": "^5.1.0",
    "@types/ramda": "^0.25.24",
    "ajv": "^6.4.0",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.4.1",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "helmet": "^3.12.0",
    "lodash": "^4.17.10",
    "memory-cache": "^0.2.0",
    "mongodb": "^3.0.7",
    "mustache-express": "^1.2.5",
    "pug": "^2.0.3",
    "query-string": "^6.1.0",
    "ramda": "^0.25.0",
    "serve-static": "^1.13.2"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.0",
    "awesome-typescript-loader": "^5.0.0",
    "clean-webpack-plugin": "^0.1.19",
    "mocha": "^5.1.1",
    "nsp": "^3.2.1",
    "snyk": "^1.78.1",
    "ts-node": "^6.0.3",
    "tslint": "^5.10.0",
    "typescript": "^2.8.3",
    "webpack": "^4.8.1",
    "webpack-cli": "^2.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "keywords": [
    "express",
    "framework",
    "light",
    "microframework",
    "node"
