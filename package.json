{
  "name": "lexpress",
  "version": "0.2.1",
  "description": "NodeJS + Express based light framework.",
  "engines": {
    "node": "8.4.0",
    "npm": "5.3.0"
  },
  "main": "./dist/index.js",
  "types": "./src/types.d.ts",
  "scripts": {
    "build": "rimraf ./build && webpack --display-error-details",
    "dist": "rimraf ./dist && npm run build && copyfiles -f ./build/index.js ./dist",
    "start": "node ./build/server.bundle.js",
    "test": "npm run dist && mocha -r ts-node/register test/*.ts"
  },
